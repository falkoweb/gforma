$modelname "exemple.mdl"

$bodygroup "exemple"
{
	studio "exemple.smd"
}

$surfaceprop "flesh"

$contents "solid"

$eyeposition 0 0 70

$mostlyopaque

$cdmaterials "models\exemple\"

$bonemerge "ValveBiped.Bip01_Pelvis"
$bonemerge "ValveBiped.Bip01_Spine"
$bonemerge "ValveBiped.Bip01_Spine1"
